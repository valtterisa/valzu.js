{
  "name": "@landrr/blocks",
  "version": "4.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "presets"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./presets/default.css": "./presets/default.css",
    "./presets/minimal.css": "./presets/minimal.css",
    "./presets/zinc.css": "./presets/zinc.css"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.0.0",
    "@landrr/core": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "tsup": "^8.3.5",
    "@landrr/core": "workspace:*"
  },
  "scripts": {
    "build": "tsup src/index.tsx --format cjs,esm --dts --sourcemap --clean --external react --external react-dom --external react-router-dom --external @landrr/core"
  }
}
